	"item": [
		{
			"name": "folio/api/auth",
			"item": [
				{
					"name": "login with credentials",
					"event": [
						{
							"listen": "test",
							"script": {
								"id": "3ad4d5e0-fc97-41a9-8cdd-57e76e58f0ed",
								"exec": [
									"// extract session token for user with other requests",
									"var response = pm.response.json();",
									"pm.globals.set(\"sessionToken\", response.sessionToken);"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"name": "Content-Type",
								"value": "application/json",
								"type": "text"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n\t\"EmailAddr\": \"s1234112@ap.edu.sg\",\n\t\"Password\": \"p@55Student\"\n}"
						},
						"url": {
							"raw": "http://localhost:5000/api/auth/login",
							"protocol": "http",
							"host": [
								"localhost"
							],
							"port": "5000",
							"path": [
								"api",
								"auth",
								"login"
							]
						}
					},
					"response": []
				},
				{
					"name": "check session token",
					"request": {
						"auth": {
							"type": "bearer",
							"bearer": [
								{
									"key": "token",
									"value": "{{sessionToken}}",
									"type": "string"
								}
							]
						},
						"method": "GET",
						"header": [],
						"url": {
							"raw": "http://localhost:5000/api/auth/check",
							"protocol": "http",
							"host": [
								"localhost"
							],
							"port": "5000",
							"path": [
								"api",
								"auth",
								"check"
							]
						}
					},
					"response": []
				}
			]
		},
